{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "role": {
      "type": "string"
    },
    "github": {
      "type": "string"
    },
    "twitter": {
      "type": "string"
    },
    "linkedin": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "avatar": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "name",
    "role",
    "avatar"
  ]
}
