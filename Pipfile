[[source]]
url = "https://pypi.org/simple"
verify_ssl = true
name = "pypi"

[scripts]
build = "python pyvecorg/build.py"
test = "pytest"
freeze = "python pyvecorg freeze"
serve = "python pyvecorg serve"
deploy = "python pyvecorg deploy --push"

[packages]
# Pinning packages with ~= unless their version starts with 0.,
# then pinning each version strictly with ==
flask = "~=2.0.2"
elsa = "==0.1.6"
markdown = "~=3.3"
pyyaml = "~=6.0"
requests = "~=2.26.0"
python-slugify = "~=5.0.2"
pillow = "~=8.4.0"
gspread = "~=4.0.1"
oauth2client = "~=4.1.3"

[dev-packages]
# Pinning packages with ~= unless their version starts with 0.,
# then pinning each version strictly with ==
pipfile = "==0.0.2"
pylama = "~=7.7.1"
pytest = "~=6.2.5"
jsonschema = "~=4.1.2"

[requires]
python_version = "3.9"
